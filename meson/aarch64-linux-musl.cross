[binaries]
c = '/musl/bin/aarch64-linux-musl-gcc'
cpp = '/musl/bin/aarch64-linux-musl-g++'
ar = '/musl/bin/aarch64-linux-musl-gcc-ar'
nm = '/musl/bin/aarch64-linux-musl-gcc-nm'
ld = '/musl/bin/aarch64-linux-musl-ld'
strip = '/musl/bin/aarch64-linux-musl-strip'
